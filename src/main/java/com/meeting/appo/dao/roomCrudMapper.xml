<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.meeting.appo.dao.EventRoomDao">
    <insert id="addRoom" parameterType="com.meeting.appo.entities.Room">
        INSERT INTO tb_rooms(rflood,rserial,seats,isAvailable,comm,create_date)
        VALUES(#{room.rflood},#{room.rserial},#{room.seats},#{room.isAvailable},#{room.comm},#{room.create_date})
    </insert>




</mapper>